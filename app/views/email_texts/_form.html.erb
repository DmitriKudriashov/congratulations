<%= form_with model: @email_texts, local: true do |form| %>
  <p>
    <%= 'Text: ' %>
    <%= form.collection_select :cardtext_id, Cardtext.order(:filename), :id, :filename, prompt: 'Select cardtext' %>
  </p>
  <p>
    <%= 'Email: ' %>
    <%= form.collection_select :email_id, Email.order(:name), :id, :name, prompt: 'Select Email' %>
  </p>
  <br>
  <p> <%= form.submit %> </p>
<% end %>
<br>
