<%= form_with model: [@holiday, @dates_holiday], local: true do |form| %>
  <p>
   Date: <%= date_field :dates_holiday, :date %>
  </p>
  <p>
    <%= 'Holiday: ' %>
    <%= form.collection_select :holiday_id, Holiday.all, :id, :name, prompt: 'Select holiday' %>
  </p>
  <br>
  <p> <%= form.submit %> </p>
<% end %>

