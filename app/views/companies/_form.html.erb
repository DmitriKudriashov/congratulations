<%= form_with model: @company, local: true do |form| %>
  <p>
    <%= form.label :name %>:
    <%= form.text_field :name %>
  </p>
  <p>
    <%= 'Country: ' %>
    <%= form.collection_select :country_id, Country.all, :id, :name, prompt: 'Select Country' %>
  </p>
  <p>
    <%= form.label :email%>:
    <%= form.text_field :email %>
  </p>
  <br>
  <p> <%= form.submit %> </p>
<% end %>
<br>
