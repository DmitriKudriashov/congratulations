<%#= byebug %>
<%= form_with model: @companies_holiday, local: true do |form| %>
  <p>
    <%= 'Company: ' %>
    <%= form.collection_select :company_id, Company.all, :id, :name, prompt: 'Select company' %>
  </p>
  <p>
    <%= 'Holiday: ' %>
    <%= form.collection_select :holiday_id, Holiday.all, :id, :name, prompt: 'Select holiday' %>
  </p>
  <br>
  <%#= byebug %>
  <p> <%= form.submit %> </p>
<% end %>
<br>
